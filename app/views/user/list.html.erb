<%= stylesheet_link_tag	"http://netdna.bootstrapcdn.com/twitter-bootstrap/2.3.1/css/bootstrap-combined.min.css" %>

<table class="table">
  <tr>
	<th>First Name</th>
	<th>Last Name</th>
	<th>Email</th>

  </tr>
<% if flash[:users] -%>
	<% flash[:users].each do |user| %>
	  <tr>
	    <td ><%=h user.first_name %></td>
	    <td ><%=h user.last_name %></td>
	    <td ><%=h user.email %></td>
	  </tr>
	<% end %>
<% else -%>
	<p>
		No Users are currently registered!
	</p>
<% end -%>

</table>
